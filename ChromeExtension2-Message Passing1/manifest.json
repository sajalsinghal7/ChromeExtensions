{
  "manifest_version": 2,

  "name": "Getting started example--2",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },  
  "background": {
    "scripts": ["background.js"]
  },
    "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": ["myScript.js"]
    }
  ]
}